{
    "Class": {
        "prefix": "hosted-service",
        "body": [
            "${1|hosted,external|} service ${2:Class} on ${2:Class}"
        ],
        "description": "Service declaration"
    },
    "System LogInfo": {
        "prefix": "logi",
        "body": [
            "LogInfo($0)"
        ],
        "description": "System.LogInfo"
    },
    "System LogError": {
        "prefix": "loge",
        "body": [
            "LogError($0)"
        ],
        "description": "System.LogError"
    },
    "Procedure": {
        "prefix": "procedure",
        "body": [
            "procedure ${1:Procedure}(${2:Params})",
            "\t$0"
        ],
        "description": "Procedure definition"
    },
    "Alias": {
        "prefix": "alias",
        "body": [
            "object ${1:ObjectName}:${2:ObjectType}"
        ],
        "description": "alias definition"
    },
    "Composite-Object": {
        "prefix": "object",
        "body": [
            "object ${1:ObjectName}",
            "\t${2:fieldName}:${3:FieldType}"
            "\t$0"
        ],
        "description": "poco-object definition"
    },
    "Enum": {
        "prefix": "enum",
        "body": [
            "enum ${1:EnumName}",
            "\t$0"
        ],
        "description": "enum definition"
    },
    "ForEach": {
        "prefix": "foreach",
        "body": [
            "foreach ${1:Iteratee} in ${2:Iterator}",
            "\t$0"
        ],
        "description": "ForEach definition"
    },
    "Formula": {
        "prefix": "formula",
        "body": [
            "formula To${1:Value}(${2:Params}):${1:Value} = $0"
        ],
        "description": "Formula definition"
    },
    "If": {
        "prefix": "if",
        "body": [
            "if ${1:Expression}",
            "\t$0"
        ],
        "description": "If definition"
    },
    "IfTernary": {
        "prefix": "ift",
        "body": [
            "if (${1:IfExpression}, ${2:Yes}, ${3:No})"
        ],
        "description": "If ternary definition"
    },
    "Error": {
        "prefix": "error",
        "body": [
            "error[${1:Error}] \"${2:ErrorMessage}\""
        ],
        "description": "Error definition"
    }
}
